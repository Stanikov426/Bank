      <Label layoutX="335.0" layoutY="168.0" text="Adress" />
      <Label layoutX="343.0" layoutY="209.0" text="City" />
      <Label layoutX="319.0" layoutY="250.0" text="Zip-code" />
      <Label layoutX="75.0" layoutY="172.0" text="Phone" />
      <Label layoutX="75.0" layoutY="204.0" text="E-mail" />
