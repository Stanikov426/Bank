<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.editContactController">
   <children>
      <TextField fx:id="phone" layoutX="121.0" layoutY="168.0" />
