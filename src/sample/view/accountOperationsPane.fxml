<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.accountOperationsController">
   <children>
      <Label layoutX="17.0" layoutY="14.0" text="Account operations">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TableView fx:id="operationsTable" layoutX="48.0" layoutY="63.0" prefHeight="274.0" prefWidth="483.0">
         <columns>
            <TableColumn fx:id="dateColumn" prefWidth="106.0" text="Date" />
            <TableColumn fx:id="accNumberColumn" prefWidth="116.0" text="AccNumber" />
            <TableColumn fx:id="toColumn" prefWidth="113.0" text="To" />
            <TableColumn fx:id="descriptionColumn" prefWidth="146.0" text="Description" />
         </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="14.0" layoutY="348.0" mnemonicParsing="false" onAction="#backClick" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
