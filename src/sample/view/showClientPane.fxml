<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.showClientController">
   <children>
      <Label fx:id="infoLabel" layoutX="14.0" layoutY="14.0" text="Label">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button layoutX="225.0" layoutY="138.0" mnemonicParsing="false" onAction="#addAccClick" text="Add Account">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="248.0" layoutY="248.0" mnemonicParsing="false" onAction="#showAllClick" text="Show All">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="213.0" layoutY="194.0" mnemonicParsing="false" onAction="#delAccClick" text="Delete Account">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="231.0" layoutY="302.0" mnemonicParsing="false" onAction="#editContactClick" text="Edit Contact">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="14.0" layoutY="351.0" mnemonicParsing="false" onAction="#backClick" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="adresLabel" layoutX="15.0" layoutY="44.0" text="Label">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="contactLabel" layoutX="15.0" layoutY="79.0" text="Label">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
